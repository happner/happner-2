{
  "name": "happner-2",
  "version": "9.2.2",
  "description": "distributed application engine with evented storage and mesh services",
  "main": "lib/mesh.js",
  "bin": {
    "happner": "bin/happner",
    "happner-loader": "bin/happner-loader"
  },
  "scripts": {
    "test": "npm run test-lint && mocha -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "test-bc": "mocha test/backward-compatibility/* --exit",
    "test-benchmark": "node test/benchmark/serial-run.js",
    "test-browser": "gulp --gulpfile test/browser/gulp-01.js",
    "test-unit": "_mocha --exit -- silence.js test/unit/** ",
    "test-integration": "_mocha --exit -- silence.js test/integration/** ",
    "test-cover": "istanbul cover _mocha -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "test-release": "rm -rf node_modules && rm -f package-lock.json && npm install && npm test",
    "test-010": "source ~/.nvm/nvm.sh && nvm use 0.10 && rm -rf node_modules && npm install && node_modules/mocha/bin/_mocha test/integration/**",
    "test-lint": "gulp lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happner/happner-2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happner/happner-2/issues"
  },
  "homepage": "https://github.com/happner/happner-2",
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "expect.js": "0.3.1",
    "fxt": "1.0.2",
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.1.0",
    "happn-util-crypto": "0.3.0",
    "happner": "1.33.10",
    "happner-serial-mocha": "1.5.2",
    "happner-test-modules": "1.0.2",
    "istanbul": "0.4.5",
    "jshint": "^2.9.7",
    "karma": "3.1.3",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "mocha": "5.2.0",
    "restler": "3.4.0",
    "serve-static": "1.13.2",
    "should": "13.2.3",
    "traverse": "0.6.6",
    "tree-kill": "^1.2.1",
    "when": "3.7.8"
  },
  "dependencies": {
    "async": "0.9.2",
    "bluebird": "2.10.2",
    "body": "5.1.0",
    "change-case": "3.0.2",
    "commander": "2.19.0",
    "deep-copy": "1.4.2",
    "diskspace": "2.0.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "happn-3": "git+https://github.com/happner/happn-3.git#issue/170",
    "happn-logger": "0.0.2",
    "happn-sillyname": "0.1.1",
    "happner-client": "^6.2.0",
    "happner-hyperid": "2.0.0",
    "happner-terminal": "0.0.8",
