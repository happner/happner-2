{
  "name": "happner-2",
  "version": "11.5.0",
  "description": "distributed application engine with evented storage and mesh services",
  "main": "lib/mesh.js",
  "bin": {
    "happner": "bin/happner",
    "happner-loader": "bin/happner-loader"
  },
  "nyc": {
    "exclude": [
      "test",
      "docs",
      "gulpfile.js",
      "lib/services/connect/public/browser_primus.js"
    ]
  },
  "scripts": {
    "test": "npm run lint && _mocha --exit -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "test-bc": "mocha test/backward-compatibility/*",
    "test-benchmark": "node test/benchmark/serial-run.js",
    "test-browser": "node_modules/.bin/gulp --gulpfile test/browser/gulp-01.js",
    "test-unit": "_mocha --exit -- silence.js test/unit/**",
    "test-integration": "_mocha  --exit -- silence.js test/integration/**",
    "test-cover": "npm run lint && nyc _mocha --exit -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "pre-release": "rm -rf node_modules && rm -f package-lock.json && npm install && npm run test-cover && npm run test-browser",
    "release": "rm -rf node_modules && npm install && npm run test-cover && npm run test-browser",
    "test-010": "source ~/.nvm/nvm.sh && nvm use 0.10 && rm -rf node_modules && npm install && node_modules/mocha/bin/_mocha test/integration/**",
    "lint": "eslint '**/*.js'",
    "lint-fix": "eslint '**/*.js' --fix",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "rm -rf node_modules/happn/node_modules/bitcore-lib && rm -rf node_modules/happner-client/node_modules/bitcore-lib && rm -rf node_modules/bitcore-ecies/node_modules/bitcore-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happner/happner-2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happner/happner-2/issues"
  },
  "homepage": "https://github.com/happner/happner-2",
  "devDependencies": {
    "address": "^1.1.2",
    "await-delay": "^1.0.0",
    "chai": "4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "expect.js": "0.3.1",
    "fxt": "1.0.2",
    "gulp": "^4.0.0",
    "happn-service-mongo-2": "^3.0.0",
    "happn-util-crypto": "^1.0.0",
    "happner": "1.33.4",
    "happner-serial-mocha": "^1.5.2",
    "happner-test-modules": "1.0.2",
    "is-ip": "^3.1.0",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "prettier": "^1.18.2",
    "restler": "3.4.0",
    "serve-static": "1.10.0",
    "should": "7.1.0",
    "traverse": "0.6.6",
    "tree-kill": "^1.2.1",
    "when": "3.7.3",
    "why-is-node-running": "^2.1.0"
  },
  "dependencies": {
    "async": "^3.2.0",
    "body": "5.1.0",
    "change-case": "2.3.0",
    "commander": "2.9.0",
    "deep-copy": "1.1.2",
    "diskspace": "0.1.7",
    "fs-extra": "0.24.0",
    "glob": "5.0.15",
    "happn-3": "^11.8.1",
    "happn-logger": "^1.1.1",
    "happn-sillyname": "^0.1.1",
    "happner-client": "^11.2.0",
    "happner-hyperid": "2.0.0",
    "happner-terminal": "0.0.9",
    "homedir": "0.6.0",
    "later": "1.2.0",
    "lodash": "^4.17.11",
    "md5": "2.0.0",
    "merge": "1.2.0",
    "proc-stats": "0.0.4",
    "repl.history": "0.1.4",
    "request": "^2.67.0",
    "shortid": "2.2.4",
    "uglify-es": "^3.3.9"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  }
}
