{
  "name": "happner-2",
  "version": "10.1.0",
  "description": "distributed application engine with evented storage and mesh services",
  "main": "lib/mesh.js",
  "bin": {
    "happner": "bin/happner",
    "happner-loader": "bin/happner-loader"
  },
  "nyc": {
    "exclude": [
      "test",
      "docs",
      "gulpfile.js"
    ]
  },
  "scripts": {
    "test": "npm run test-lint && _mocha --exit -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "test-bc": "mocha test/backward-compatibility/*",
    "test-benchmark": "node test/benchmark/serial-run.js",
    "test-browser": "node_modules/.bin/gulp --gulpfile test/browser/gulp-01.js",
    "test-unit": "_mocha --exit -- silence.js test/unit/**",
    "test-integration": "_mocha  --exit -- silence.js test/integration/**",
    "test-cover": "nyc _mocha --exit -- silence.js test/integration/** test/unit/** test/backward-compatibility/**",
    "release": "rm -rf node_modules && rm -f package-lock.json && npm install && npm run test-cover && npm run test-browser",
    "test-010": "source ~/.nvm/nvm.sh && nvm use 0.10 && rm -rf node_modules && npm install && node_modules/mocha/bin/_mocha test/integration/**",
    "test-lint": "gulp lint",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happner/happner-2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happner/happner-2/issues"
  },
  "homepage": "https://github.com/happner/happner-2",
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "^3.0.2",
    "expect.js": "0.3.1",
    "fxt": "1.0.2",
    "gulp": "3.9.0",
    "gulp-jshint": "^2.1.0",
    "happn-util-crypto": "^0.3.0",
    "happner": "1.33.4",
    "happner-serial-mocha": "^1.5.2",
    "happner-test-modules": "1.0.2",
    "jshint": "^2.9.5",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "restler": "3.4.0",
    "serve-static": "1.10.0",
    "should": "7.1.0",
    "traverse": "0.6.6",
    "tree-kill": "^1.2.1",
    "when": "3.7.3",
    "why-is-node-running": "^2.0.3"
  },
  "dependencies": {
    "async": "0.9.2",
    "bluebird": "2.10.2",
    "body": "5.1.0",
    "change-case": "2.3.0",
    "commander": "2.9.0",
    "deep-copy": "1.1.2",
    "diskspace": "0.1.7",
    "fs-extra": "0.24.0",
    "glob": "5.0.15",
    "happn-3": "^10.0.0",
    "happn-logger": "^0.1.1",
    "happn-sillyname": "0.1.1",
    "happner-client": "^8.0.0",
    "happner-hyperid": "2.0.0",
    "happner-terminal": "0.0.8",
    "homedir": "0.6.0",
    "later": "1.2.0",
    "lodash": "^4.17.11",
    "md5": "2.0.0",
    "merge": "1.2.0",
    "proc-stats": "0.0.4",
    "repl.history": "0.1.4",
    "request": "2.67.0",
    "shortid": "2.2.4",
    "uglify-js": "2.6.1"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  }
}
